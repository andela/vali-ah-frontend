{
  "name": "vali-ah-frontend",
  "version": "1.0.0",
  "description": "The one-stop shop for all the self-help ideas you need.",
  "main": "index.jsx",
  "scripts": {
    "start": "node ./app",
    "start:dev": "webpack-dev-server --hot --open --config ./config/webpack.dev.js",
    "build": "webpack --config ./config/webpack.prod.js",
    "lint:js": "eslint '**/*.js' --fix",
    "lint:jsx": "eslint '**/*.jsx' --fix",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "npm run build && npm run test:e2e",
    "test:e2e:ci": "npm run test:e2e --parallel",
    "test:unit": "jest",
    "test:unit:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela/vali-ah-frontend.git"
  },
  "keywords": [
    "1kbideas",
    "positivity",
    "community"
  ],
  "author": "Team-vali",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela/vali-ah-frontend/issues"
  },
  "homepage": "https://github.com/andela/vali-ah-frontend#readme",
  "dependencies": {
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.12",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@hot-loader/react-dom": "^16.9.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-storyshots": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "cypress": "^3.4.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "2.4.1",
    "jest": "^24.9.0",
    "jest-junit": "^7.0.0",
    "lint-staged": "^9.2.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react-test-renderer": "^16.9.0",
    "require-context.macro": "^1.1.1",
    "start-server-and-test": "^1.10.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.4.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:unit && npm run test:e2e:dev",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
